{
  "name" : "rampage-php/nexus-deployment-manager",
  "description" : "A software to manage your PHP application deployments",
  "require" : {
    "zendframework/zendframework" : "2.*",
    "rampage-php/framework" : ">= 1.2,<2",
    "rampage-php/auth" : "1.0.*",
    "doctrine/doctrine-mongo-odm-module" : "<1.1",
    "doctrine/mongodb-odm" : "1.0.*@beta",
    "rampage-php/gui" : "1.0.*@dev"
  },
  "repositories" : [ {
    "type" : "git",
    "url" : "https://github.com/tux-rampage/rampage-gui"
  } ],
  "require-dev" : {
    "rampage-php/framework" : "1.2.*@dev"
  },
  "authors" : [ {
    "name" : "Axel Helmert",
    "email" : "staff@pro-unreal.de",
    "role" : "lead"
  } ],
  "keywords" : [ "rampage", "saas" ],
  "autoload" : {
    "psr-4" : {
      "rampage\\nexus\\" : "application/modules/rampage.nexus/src/",
      "rampage\\nexus\\zs\\" : "application/modules/rampage.nexus.zendserver/src/"
    }
  },
  "config" : {
    "discard-changes" : true
  },
  "type" : "application",
  "license" : "GPL-3.0+",
  "homepage" : "https://github.com/tux-rampage/rampage-nexus"
}